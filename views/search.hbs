<h2>{{title}}</h2>
<div class="searchResultsContainer w-100 col-xs-12 col-sm-12 excerpet">
  {{#if searchResult}}
    {{#each searchResult}}
      <div class="srItem w-100">
        <h3><a href="#">{{this.cat1}} -> {{this.cat2}} -> {{this.cat3}} -> pg{{this.pg}} </a></h3>
        <p>... {{this.ts_headline}} ...</p>
